{
  "extends": "airbnb-base",
  "plugins": ["import"],
  "rules": {
    "semi": "off",
    "max-len": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "prefer-destructuring": "off",
    "camelcase": "off",
    "comma-dangle": "off",
    "consistent-return": "off",
    "no-await-in-loop": "off",
    "no-plusplus": "off",
    "import/no-dynamic-require": "off",
    "no-param-reassign": [
      "off",
      {
        "props": false
      }
    ]
  }
}
